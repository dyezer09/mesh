                  <h4 class="bg-dark text-primary p-2 m-0">${{this.cost}}</h4>
                </div>
                <div class="d-flex flex-column justify-content-center text-start bg-secondary border-inner px-4">
                  <h5 class="text-uppercase">{{this.name}}</h5>
                  <span>Ipsum ipsum clita erat amet dolor sit justo sea eirmod diam stet sit
                    justo</span>
                </div>
              </div>
            </div>
            {{/each}}
          </div>
        </div>
        <div id="tab-3" class="tab-pane fade show p-0">
          <div class="row g-3">
            {{#each tab3}}
            <div class="col-lg-6">
              <div class="d-flex h-100">
                <div class="flex-shrink-0">
                  <img class="img-fluid" src="img/cake-3.jpg" alt="" style="width: 150px; height: 85px;">
                  <h4 class="bg-dark text-primary p-2 m-0">${{this.cost}}</h4>
                </div>
                <div class="d-flex flex-column justify-content-center text-start bg-secondary border-inner px-4">
                  <h5 class="text-uppercase">{{this.name}}</h5>
                  <span>Ipsum ipsum clita erat amet dolor sit justo sea eirmod diam stet sit
                    justo</span>
                </div>
              </div>
            </div>
            {{/each}}
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
