<!-- Products Start -->
<div class="container-fluid about py-5">
  <div class="container">
    <div class="section-title position-relative text-center mx-auto mb-5 pb-3" style="max-width: 600px;">
      <h2 class="text-primary font-secondary">Мы выполним для Вас </h2>
      <h3 class="display-4 text-uppercase">штукарные работы в Гомеле и
        Гомельской области</h3>
    </div>
    <div class="tab-class text-center">
      <ul class="nav nav-pills d-inline-flex justify-content-center bg-dark text-uppercase border-inner p-4 mb-5">
        <li class="nav-item">
          <a class="nav-link text-white active" data-bs-toggle="pill" href="#tab-1">штукатурка стен</a>
        </li>
        <li class="nav-item">
          <a class="nav-link text-white" data-bs-toggle="pill" href="#tab-2">штукатурка потолков</a>
        </li>
        <li class="nav-item">
          <a class="nav-link text-white" data-bs-toggle="pill" href="#tab-3">штукатурка откосов</a>
        </li>
      </ul>
      <div class="tab-content">
        <div id="tab-1" class="tab-pane fade show p-0 active">
          <div class="row g-3">
            {{#each tab1}}
            <div class="col-lg-6">
              <div class="d-flex h-100">
                <div class="flex-shrink-0">
                  <img class="img-fluid" src="img/cake-1.jpg" alt="" style="width: 150px; height: 85px;">
                  <h4 class="bg-dark text-primary p-2 m-0">от {{this.cost}} руб/кв.м</h4>
                </div>
                <div class="d-flex flex-column justify-content-center text-start bg-secondary border-inner px-4">
                  <h5 class="text-uppercase">{{this.name}}</h5>
                  <span>Ipsum ipsum clita erat amet dolor sit justo sea eirmod diam stet sit
                    justo</span>
                </div>
              </div>
            </div>
            {{/each}}
          </div>
        </div>
        <div id="tab-2" class="tab-pane fade show p-0">
          <div class="row g-3">
            {{#each tab2}}
            <div class="col-lg-6">
              <div class="d-flex h-100">
